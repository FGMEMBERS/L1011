<PropertyList>
 

 <path>Aircraft/L1011/Models/RR-RB211-22.ac</path>

<!-- ................................................... -->
<!-- Engines -->
<!-- ................................................... -->


  <animation>
  <type>material</type>
   <object-name>engines_RR</object-name>
   <property-base>sim/model/livery</property-base>
   <texture-prop>texture</texture-prop>
   <texture>Liveries/LTU90.png</texture>
 </animation>


<animation>
  <type>spin</type>
  <object-name>engine_RR_fan_r</object-name>
   <property>/engines/engine[0]/n1</property>
  <factor>-2</factor>
  <center>
   <x-m>-0.57102</x-m>
   <y-m>10.57785</y-m>
   <z-m>-2.32696</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>spin</type>
  <object-name>engine_RR_fan_l</object-name>
   <property>/engines/engine[1]/n1</property>
  <factor>-2</factor>
  <center>
   <x-m>-0.57102</x-m>
   <y-m>-10.57785</y-m>
   <z-m>-2.32696</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
  <type>spin</type>
  <object-name>engine_RR_fan_c</object-name>
   <property>/engines/engine[1]/n1</property>
  <factor>-2</factor>
  <center>
   <x-m>24.08202</x-m>
   <y-m>0</y-m>
   <z-m>1.83811</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
<animation>
 <type>select</type>
 <object-name>engine_RR_fan_r</object-name>  
 <object-name>engine_RR_fan_l</object-name>  
 <condition>
  <less-than>
   <property>/engines/engine[0]/n1</property>
   <value>50</value>
  </less-than>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>engine_RR_fan_r_blur</object-name>
 <object-name>engine_RR_fan_l_blur</object-name>
 <condition>
  <greater-than>
   <property>/engines/engine[0]/n1</property>
   <value>50</value>
  </greater-than>
 </condition>
</animation>


</PropertyList>