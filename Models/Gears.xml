<!--Retract-->
<!--Gear Front-->
<animation>
 <type>rotate</type>
 <object-name>gear_front</object-name>  
 <object-name>gear_front_leg</object-name> 
 <property>gear/gear[0]/position-norm</property>  
 <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>110</dep>
   </entry>
   <entry>
    <ind>0.1</ind>
    <dep>110</dep>
   </entry>
   <entry>
    <ind>0.8</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>0</dep>
   </entry>
 </interpolation>
 <center>
  <x-m>-16.33728</x-m>
  <y-m>0.0</y-m>
  <z-m>-1.86608</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

