<!--Front-->
<animation>
	<type>spin</type>
	<object-name>gear_front</object-name>
	<property>gear/gear[0]/rollspeed-ms</property>
	<factor>10</factor>
	<center>
		<x-m>-16.35296</x-m>
		<y-m>0</y-m>
		<z-m>-4.06968</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>-1</y>
		<z>0</z>
	</axis>
</animation>

